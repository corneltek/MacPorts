PortSystem 1.0

name		desproxy
version		0.1.0-pre1
categories	net
maintainers	mww@opendarwin.org
description	tunnels tcp, as socks, etc. through an http-proxy
long_description	Desproxy makes you able to stablish TCP connections \
			through HTTP proxies

homepage	http://www
master_sites	sourceforge:${name}
extract.sufx	.tgz
checksums	md5 2d3c02fe0fd2d191da6963ab8d920fea
patchfiles	patch-Makefile.in.diff

depends_lib	lib:libintl:gettext

set worksrcpath	${workpath}/${name}

build.dir	${worksrcpath}

destroot.cmd	cp src/desproxy src/desproxy-inetd src/desproxy-dns src/desproxy-socks5server src/desproxy-socks4server src/socket2socket ${destroot}${prefix}/bin
destroot.target
destroot.destdir

