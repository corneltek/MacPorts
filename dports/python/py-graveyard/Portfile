# $Id$

PortSystem  1.0

name        py-graveyard
version     0.0
categories  python

array set replacements {
    24 27
    25 27
    26 27
    31 34
    32 34
    33 34
}

proc unknown args {
    upvar 1 replacements replacements
    foreach series [lassign $args port vers] {
        lassign [split $vers _] ver rev
        if {$rev eq ""} {
            set rev 0
        }
        set repl $replacements($series)
        subport py${series}-$port {
            version $ver
            revision $rev
            replaced_by py${repl}-$port
        }
    }
}

