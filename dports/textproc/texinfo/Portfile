                ftp://mirrors.kernel.org/gnu/${name}/ \
	        http://mirrors.kernel.org/gnu/${name}/ \
	        ftp://gatekeeper.dec.com/pub/GNU/${name}/ \
	        ftp://ftp.keystealth.org/pub/gnu/${name}/
